{
    "name": "@tb/monorepo",
    "private": true,
    "workspaces": [
        "./apps/*",
        "./packages/*"
    ],
    "engines": {
        "node": ">=16",
        "npm": ">=7"
    },
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev & npm run serve",
        "serve": "node -r esm server",
        "start": "turbo run start",
        "psotDev": "npm run serve",
        "lint": "turbo run lint",
        "kill": "kill $(lsof -t -i :3000-3011)",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "@vercel/nft": "^0.22.6",
        "eslint": "^7.32.0",
        "eslint-config-custom": "@tbweb/eslint-config-custom",
        "prettier": "^2.5.1",
        "turbo": "latest"
    },
    "dependencies": {
        "dd-trace": "^3.9.3",
        "esm": "^3.2.25",
        "express": "^4.18.2",
        "express-http-proxy": "^1.6.3",
        "http-proxy-middleware": "^2.0.6"
    }
}
