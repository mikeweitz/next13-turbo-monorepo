{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [
        "$PRODUCTS_URL"
    ],
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "dist/**"
            ]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        }
    }
}
