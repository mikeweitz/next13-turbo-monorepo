{
    "$schema": "https://turbo.build/schema.json",
    "globalEnv": [
        "PRODUCTS_URL",
        "PORT"
    ],
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "dist/**"
            ]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "start": {
            "dependsOn": [
                "^start"
            ]
        }
    }
}
